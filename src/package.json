{
  "name": "nautilus-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:ts": "$(npm bin)/tsc",
    "build:webpack": "$(npm bin)/webpack --config assets/app/scripts/webpack.config.js",
    "build": "npm run build:ts && npm run build:webpack",
    "clean:ts": "rm -rf ./out",
    "clean:webpack": "rm -f ./assets/app/scripts/main.bundle.js ./assets/app/scripts/main.bundle.js.map",
    "clean": "npm run clean:ts && npm run clean:webpack",
    "start": "node ./out/www.js",
    "watch": "nodemon -i assets ./out/www.js"
  },
  "dependencies": {
    "async": "^1.4.2",
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "express-session": "^1.11.3",
    "jade": "~1.6.0",
    "mongodb": "^2.0.40",
    "morgan": "~1.3.0",
    "nconf": "^0.7.2",
    "node-uuid": "~1.4.2",
    "passport": "^0.2.2",
    "passport-local": "^1.0.0",
    "request": "^2.72.0",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "underscore": "~1.8.2",
    "webpack": "^1.13.1"
  }
}
