{
  "name": "nautilus-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:server": "$(npm bin)/tsc",
    "build:client": "$(npm bin)/webpack",
    "build": "npm run build:server && npm run build:client",
    "clean:server": "rm -rf ./out",
    "clean:client": "rm -f ./assets/app/scripts/main.bundle.js ./assets/app/scripts/main.bundle.js.map",
    "clean": "npm run clean:server && npm run clean:client",
    "start:server": "node ./out/www.js",
    "start": "npm run start:server",
    "watch:server": "nodemon -e ts -i ./assets/app -x 'npm run build:server && npm run start:server'",
    "watch:client": "nodemon -e ts,tsx -w ./assets/app -x 'npm run build:client'"
  },
  "dependencies": {
    "async": "^1.4.2",
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "jade": "~1.6.0",
    "mongodb": "^2.0.40",
    "morgan": "~1.3.0",
    "nconf": "^0.7.2",
    "node-uuid": "~1.4.2",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "underscore": "~1.8.2",
    "webpack": "^1.13.1",
    "webpack-fail-plugin": "^1.0.5"
  }
}
